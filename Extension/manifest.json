{
  "manifest_version": 2,

  "name": "Overtime Manager",
  "description": "Never miss your overtime details.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon64.png",
    "default_popup": "overtime.html"
  },

  "icons":{
    "16":"icon16.png",
    "64":"icon64.png",
    "128":"icon128.png"
  },

  "permissions": [
    "activeTab",
    "storage",
    "alarms"
  ]
}
